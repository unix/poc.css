{
  "$schema": "http://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "printWidth": 85,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "singleQuote": true,
  "bracketSameLine": true,
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
